{
  "version": 2,
  "projects": {
    "api": "apps/api",
    "api-util-notifications": "libs/api/util-notifications",
    "store": "apps/store",
    "store-e2e": "apps/store-e2e",
    "store-feature-game-detail": "libs/store/feature-game-detail",
    "store-ui-shared": "libs/store/ui-shared",
    "store-ui-shared-e2e": "apps/store-ui-shared-e2e",
    "store-util-formatters": "libs/store/util-formatters",
    "util-interface": "libs/util-interface"
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "css"
    }
  },
  "defaultProject": "store"
}
